public class Divisao extends Operacao {
    public Divisao(double num1, double num2) {
        super(num1, num2);
    }

    @Override
    public double executar() {
        if (num2 == 0) {
            System.out.println("Erro: divis√£o por zero!");
            return Double.NaN;
        }
        return num1 / num2;
    }

    @Override
    public String toString() {
        return num1 + " / " + num2;
    }
}
